{
  "name": "@goji/webpack-plugin",
  "version": "0.8.4",
  "description": "GojiJS Webpack Plugin",
  "main": "dist/cjs/cjs.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "start": "tsc --watch",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "test": "jest",
    "demo": "(cd demo && webpack)"
  },
  "jest": {
    "testMatch": [
      "**/src/**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "cacheDirectory": "/tmp/.cache/jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "./babel-jest.config.js"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@goji/core": "^0.8.4",
    "@types/ejs": "^3.0.4",
    "@types/enhanced-resolve": "^3.0.6",
    "@types/html-minifier": "^4.0.0",
    "@types/loader-utils": "^2.0.1",
    "@types/lodash": "^4.14.151",
    "@types/node": "^14.14.7",
    "@types/replace-ext": "^2.0.0",
    "@types/resolve": "^1.17.1",
    "@types/webpack": "^4.41.13",
    "@types/webpack-sources": "^0.1.7",
    "jest": "^26.0.1",
    "rimraf": "^3.0.0",
    "typescript": "^4.0.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "copy-webpack-plugin": "^6.0.2",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.3",
    "enhanced-resolve": "^4.1.1",
    "file-loader": "^6.0.0",
    "html-minifier": "^4.0.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.15",
    "replace-ext": "^2.0.0",
    "resolve": "^1.17.0",
    "terser": "^5.3.8",
    "tslib": "^2.0.3",
    "unified": "^9.0.0",
    "webpack-sources": "^1.4.3",
    "wx2swan": "^1.2.20"
  },
  "peerDependencies": {
    "@goji/core": "*",
    "webpack": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airbnb/goji.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airbnb/goji/issues"
  },
  "homepage": "https://github.com/airbnb/goji#readme",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6b5e09e727c913f4f62dcba87df0ee2332e65099"
}
