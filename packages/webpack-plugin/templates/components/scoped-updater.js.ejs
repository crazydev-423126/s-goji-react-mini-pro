Component({
  options: {
    addGlobalClass: true,
  },
  properties: {
    gojiId: {
      type: Number,
    },
    c: {
      type: Object,
    },
  },
  methods: {
    e(evt) {
      Object.e.trigger(evt);
    },
  },
  lifetimes: {
    attached() {
      Object.e.scopedUpdaterAttach(this.properties.gojiId, this);
      Object.e.subtreeAttached(this.properties.gojiId, this);
    },
    detached() {
      Object.e.scopedUpdaterDetached(this.properties.gojiId);
      Object.e.subtreeDetached(this.properties.gojiId);
    },
  },
});
